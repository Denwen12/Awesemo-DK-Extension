{
    "name": "Awesemo DK Extension",
    "version": "1.0",
    "description": "Hand-build strong lineups using Awesemo data!",
    "manifest_version": 2,
    "content_scripts": [{
        "js": ["content.js", "assets/scripts/jquery-3.2.1.min.js", "assets/scripts/bootstrap.min.js" ],
        "css": ["styles.css", "assets/scripts/bootstrap.min.css"],
        "matches": ["https://www.draftkings.com/draft/contest/*", "https://www.draftkings.com/draft/entry/*"]
    }],
    "web_accessible_resources": [
        "assets/*"
    ]
  }